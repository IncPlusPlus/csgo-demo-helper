    "@types/node": "^15.6.1",
    "@types/uuid": "^8.3.0",
    "@types/wait-on": "^5.2.0",
    "axios": "^0.21.0",
    "ini": "^1.3.6",
    "log4js": "^6.3.0",
    "p-defer": "^3.0.0",
    "uuid": "^8.3.2",
    "wait-on": "^5.2.1"
  },
  "preferGlobal": true,
