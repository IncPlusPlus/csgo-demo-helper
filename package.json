{
  "name": "csgo-demo-helper",
  "version": "1.1.0",
  "bin": {
    "csgo-demo-helper": "dist/Main.js"
  },
  "description": "A CLI tool to assist with managing CS:GO demos",
  "main": "dist/Main.js",
  "types": "dist/Main.d.ts",
  "scripts": {
    "prepublish": "node node_modules/typescript/bin/tsc",
    "prestart": "node node_modules/typescript/bin/tsc",
    "start": "echo \"Starting CS:GO Demo Helper\" && node dist/Main.js",
    "pretest": "node node_modules/typescript/bin/tsc",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "coverage": "nyc -r text -e .ts -x \"test/**/*.test.ts\" npm run test"
  },
  "keywords": [
    "counter-strike",
    "cs:go",
    "csgo",
    "demo",
    "demos",
    "replay",
    "voice",
    "loopback",
    "voice_loopback",
    "mute"
  ],
  "author": "Ryan Cloherty",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/IncPlusPlus/csgo-demo-helper.git"
  },
  "dependencies": {
    "@types/ini": "^1.3.30",
    "@types/node": "^14.14.7",
    "@types/uuid": "^8.3.0",
    "@types/wait-on": "^5.2.0",
    "axios": "^0.21.0",
    "ini": "^1.3.5",
    "log4js": "^6.3.0",
    "p-defer": "^3.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "uuid": "^8.3.1",
    "wait-on": "^5.2.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mitm": "^1.3.3",
    "@types/mocha": "^8.0.2",
    "@types/mock-fs": "^4.13.0",
    "chai": "^4.2.0",
    "mitm": "^1.7.1",
    "mocha": "^8.1.1",
    "mock-fs": "^4.13.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.2",
    "ts-mock-imports": "^1.3.1"
  }
}
